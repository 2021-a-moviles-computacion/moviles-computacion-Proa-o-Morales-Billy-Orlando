<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/txt_nombre"
        android:layout_width="99dp"
        android:layout_height="33dp"
        android:layout_marginTop="92dp"
        android:hint="Nombre"
        app:layout_constraintEnd_toStartOf="@+id/txt_cedula"
        app:layout_constraintHorizontal_bias="0.397"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_cedula"
        android:layout_width="93dp"
        android:layout_height="38dp"
        android:layout_marginTop="92dp"
        android:layout_marginEnd="108dp"
        android:hint="Cedula"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_like"
        android:layout_width="27dp"
        android:layout_height="30dp"
        android:layout_marginTop="76dp"
        android:hint="0"
        app:layout_constraintEnd_toEndOf="@+id/txt_nombre"
        app:layout_constraintHorizontal_bias="0.256"
        app:layout_constraintStart_toStartOf="@+id/txt_nombre"
        app:layout_constraintTop_toBottomOf="@+id/txt_nombre" />

    <Button
        android:id="@+id/bt_like"
        android:layout_width="136dp"
        android:layout_height="73dp"
        android:layout_marginTop="56dp"
        android:text="DAR LIKE "
        app:layout_constraintEnd_toEndOf="@+id/txt_cedula"
        app:layout_constraintHorizontal_bias="0.348"
        app:layout_constraintStart_toStartOf="@+id/txt_cedula"
        app:layout_constraintTop_toBottomOf="@+id/txt_cedula" />
</androidx.constraintlayout.widget.ConstraintLayout>