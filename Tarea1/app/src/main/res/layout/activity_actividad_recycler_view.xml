<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActividadRecyclerView">

    <TextView
        android:id="@+id/txt_like_act_recy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="108dp"
        android:hint="0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recy_entrenador"
        android:layout_width="384dp"
        android:layout_height="367dp"
        android:layout_marginTop="48dp"
        app:layout_constraintEnd_toEndOf="@+id/txt_like_act_recy"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="@+id/txt_like_act_recy"
        app:layout_constraintTop_toBottomOf="@+id/txt_like_act_recy" />
</androidx.constraintlayout.widget.ConstraintLayout>